<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="imageView" fitHeight="362.0" fitWidth="571.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="PutDownButton" layoutX="490.0" layoutY="335.0" mnemonicParsing="false" onAction="#pressButton" text="Put Down" />
      <Button fx:id="PickUpButton" layoutX="490.0" layoutY="298.0" mnemonicParsing="false" onAction="#pressPickUpButton" prefHeight="25.0" prefWidth="68.0" text="Pick Up " />
      <Button fx:id="ForwardButton" layoutX="265.0" layoutY="237.0" mnemonicParsing="false" onAction="#pressForwardButton" prefHeight="25.0" prefWidth="68.0" text="Forward" />
      <Button fx:id="RightButton" layoutX="333.0" layoutY="285.0" mnemonicParsing="false" onAction="#pressRightButton" prefHeight="25.0" prefWidth="68.0" text="Right" />
      <Button fx:id="LeftButton" layoutX="197.0" layoutY="285.0" mnemonicParsing="false" onAction="#pressLeftButton" prefHeight="25.0" prefWidth="68.0" text="Left" />
      <ImageView fx:id="itemView" fitHeight="112.0" fitWidth="84.0" layoutX="482.0" layoutY="138.0" pickOnBounds="true" preserveRatio="true" />
      <TextField fx:id="MessageField" layoutX="225.0" layoutY="323.0" onAction="#updateMessageField" />
      <ChoiceBox fx:id="IremsCarried" layoutX="485.0" layoutY="262.0" prefHeight="25.0" prefWidth="78.0" />
      <MenuButton layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="New World">
        <items>
          <MenuItem fx:id="ICWorld" mnemonicParsing="false" onAction="#doStartNewWorld" text="ICWorld" />
          <MenuItem mnemonicParsing="false" text="(In Development)" />
        </items>
      </MenuButton>
   </children>
</AnchorPane>
